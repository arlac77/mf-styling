{
  "name": "mf-styling",
  "version": "0.0.0-semantic-release",
  "files": [
    "button.css",
    "card.css",
    "color-primary-cyan.css",
    "color-primary-fuchsia.css",
    "color-primary-inigo.css",
    "color-primary-lime.css",
    "color-primary-rose.css",
    "color-primary-teal.css",
    "color.css",
    "dropdown.css",
    "form.css",
    "global.css",
    "layout.css",
    "log.css",
    "nav.css",
    "table.css"
  ],
  "description": "styling for mf webapps",
  "keywords": [
    "vite"
  ],
  "scripts": {
    "start": "vite",
    "lint:css": "stylelint *.css",
    "lint": "npm run lint:css",
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview"
  },
  "devDependencies": {
    "stylelint": "^14.2.0",
    "stylelint-config-standard": "^24.0.0",
    "vite": "^2.7.10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/mf-styling.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/mf-styling/issues"
  },
  "homepage": "https://github.com/arlac77/mf-styling#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-css",
      "arlac77/template-vite"
    ]
  },
  "pkg": {
    "nginx.sites.dir": "/etc/nginx/sites/common",
    "csp.endpoint": "/services/reports/csp",
    "installdir": "/services/${name}",
    "group": "frontend",
    "output": {
      "arch": {}
    }
  }
}
